{
  "name": "vite-vue3-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cz-customizable": "^6.3.0",
    "nprogress": "^0.2.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "dart-sass": "^1.25.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "mrm": "^3.0.10",
    "prettier": "^2.5.1",
    "sass": "^1.47.0",
    "vite": "^2.7.2",
    "vite-plugin-compression": "^0.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "yarn lint",
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,vue,js,jsx}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
